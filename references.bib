	file = {Patel et al 2021 Projected shifts in loggerhead thermal habitat in NW Atlantic.pdf:C\:\\Users\\tomo.eguchi\\Documents\\Library\\Reprints\\Patel et al 2021 Projected shifts in loggerhead thermal habitat in NW Atlantic.pdf:application/pdf},
