	pages = {502352},
	file = {Murphy et al 2021 Life history parameters of harbor porpoise in NE Atlantic.pdf:C\:\\Users\\tomo.eguchi\\Documents\\Library\\Reprints\\Murphy et al 2021 Life history parameters of harbor porpoise in NE Atlantic.pdf:application/pdf},
