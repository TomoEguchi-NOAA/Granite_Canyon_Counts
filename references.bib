@article{raoult_operational_2020,
	title = {Operational {Protocols} for the {Use} of {Drones} in {Marine} {Animal} {Research}},
	volume = {4},
	issn = {2504-446X},
	url = {https://www.mdpi.com/2504-446X/4/4/64},
	doi = {10.3390/drones4040064},
	abstract = {The use of drones to study marine animals shows promise for the examination of numerous aspects of their ecology, behaviour, health and movement patterns. However, the responses of some marine phyla to the presence of drones varies broadly, as do the general operational protocols used to study them. Inconsistent methodological approaches could lead to diﬃculties comparing studies and can call into question the repeatability of research. This review draws on current literature and researchers with a wealth of practical experience to outline the idiosyncrasies of studying various marine taxa with drones. We also outline current best practice for drone operation in marine environments based on the literature and our practical experience in the ﬁeld. The protocols outlined herein will be of use to researchers interested in incorporating drones as a tool into their research on marine animals and will help form consistent approaches for drone-based studies in the future.},
