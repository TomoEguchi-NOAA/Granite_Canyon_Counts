	file = {Cuevas et al 2020 Spatial distribution of male turtles in Gulf of Mexico.pdf:C\:\\Users\\tomo.eguchi\\Documents\\Library\\Reprints\\Cuevas et al 2020 Spatial distribution of male turtles in Gulf of Mexico.pdf:application/pdf},
