@article{moreira_occurrence_2020,
	title = {Occurrence of {Omura}’s whale, \textit{{Balaenoptera} omurai} ({Cetacea}: {Balaenopteridae}), in the {Equatorial} {Atlantic} {Ocean} based on {Passive} {Acoustic} {Monitoring}},
	volume = {101},
