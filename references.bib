	month = nov,
	year = {2022},
	keywords = {Antarctic Peninsula, Benthos, Community composition, Food availability, Macrofauna, Meiofauna, Sea-ice cover, Weddell Sea},
	pages = {13--37},
	file = {Full Text PDF:C\:\\Users\\tomo.eguchi\\Zotero\\storage\\NTYEXKEU\\Säring et al. - 2022 - Sea-ice-related environmental drivers affect meiof.pdf:application/pdf;Snapshot:C\:\\Users\\tomo.eguchi\\Zotero\\storage\\K58VQHHA\\p13-37.html:text/html},
