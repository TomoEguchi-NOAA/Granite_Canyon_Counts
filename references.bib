	pages = {2491--2507},
	file = {Punt et al 2020 Robustness of potential biological removal to monitoring environmental and management uncertainties.pdf:C\:\\Users\\tomo.eguchi\\Documents\\Library\\Reprints\\Punt et al 2020 Robustness of potential biological removal to monitoring environmental and management uncertainties.pdf:application/pdf},
